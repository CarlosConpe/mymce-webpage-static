{
    "extends": [
        "html-validate:recommended"
    ],
    "rules": {
        "document-title": "error",
        "meta-refresh": "error",
        "missing-doctype": "error",
        "no-dup-id": "error",
        "no-inline-style": "off",
        "no-redundant-role": "error",
